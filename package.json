{
  "name": "@oqton/redux-black-box",
  "version": "1.1.2",
  "description": "Declare side effects as black boxes in redux: an alternative to redux-thunk, redux-saga, redux-loop, ...",
  "keywords": [
    "redux",
    "middleware",
    "side effects"
  ],
  "license": "MIT",
  "bugs": "https://github.com/oqton/redux-black-box/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/oqton/redux-black-box.git"
  },
  "main": "lib.js",
  "scripts": {
    "test": "node scripts/test.js"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "bluebird": "^3.5.3",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "express": "^4.16.4",
    "fetch-mock": "^7.2.5",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0",
    "react-app-polyfill": "^0.1.3",
    "redux": "^4.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  }
}
